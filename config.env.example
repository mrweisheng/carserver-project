# 数据库配置
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=car_info_db
DB_USER=root
DB_PASSWORD=1qaz!QAZ2wsx@WSX

# 服务器配置
PORT=3000
NODE_ENV=development

# JWT配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=24h

# 验证码配置
CAPTCHA_ENABLED=true
CAPTCHA_EXPIRE_MINUTES=5
CAPTCHA_MAX_ATTEMPTS=3

# 反爬虫配置 - 基础频率限制
RATE_LIMIT_BASIC_WINDOW_MS=60000
RATE_LIMIT_BASIC_MAX_REQUESTS=100

# 反爬虫配置 - 严格频率限制
RATE_LIMIT_STRICT_WINDOW_MS=60000
RATE_LIMIT_STRICT_MAX_REQUESTS=30

# 反爬虫配置 - 登录频率限制
RATE_LIMIT_LOGIN_WINDOW_MS=300000
RATE_LIMIT_LOGIN_MAX_REQUESTS=5

# 反爬虫配置 - 注册频率限制
RATE_LIMIT_REGISTER_WINDOW_MS=3600000
RATE_LIMIT_REGISTER_MAX_REQUESTS=3

# 爬虫检测配置
CRAWLER_DETECTION_ENABLED=true
CRAWLER_STRICT_IP_LIMIT=true
CRAWLER_MAX_REQUESTS_PER_MINUTE=60

# 动态延迟配置
DYNAMIC_DELAY_ENABLED=true
DYNAMIC_DELAY_MIN_MS=100
DYNAMIC_DELAY_MAX_MS=500

# 爬虫最小请求间隔
CRAWLER_MIN_REQUEST_INTERVAL_MS=1000
